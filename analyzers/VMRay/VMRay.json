{
    "name": "VMRay",
    "license": "AGPL-V3",
    "author": "Nils Kuhnert, CERT-Bund",
    "url": "https://github.com/BSI-CERT-Bund/cortex-analyzers",
    "version": "2.0",
    "description": "VMRay Sandbox file analysis.",
    "dataTypeList": ["hash", "file"],
    "command": "VMRay/vmray.py",
    "baseConfig": "VMRay",
    "config": {
    },
    "configurationItems": [
      {
        "name": "check_tlp",
        "description": "Define if the analyzer should check TLP of data before running",
        "type": "boolean",
        "multi": false,
        "required": true,
        "defaultValue": false
      },
      {
        "name": "max_tlp",
        "description": "Define the maximum TLP level autorized",
        "type": "number",
        "multi": false,
        "required": true,
        "defaultValue": 1
    },
    {
      "name": "cert",
      "description": "Verify certificate",
      "type": "boolean",
      "multi": false,
      "required": true,
      "defaultValue": true
    }
    ]
}
