{
  "name": "Cymon_Check_IP",
  "version": "2.0",
  "author": "Julian Gonzalez",
  "url": "https://github.com/ST2labs/Analyzers",
  "license": "AGPL-V3",
  "description": "Cymon.io Database Search / Report for IP",
  "dataTypeList": [
    "ip"
  ],
  "command": "docker run -i cortex-analyzers-cymon python cymon_analyzer.py",
  "baseConfig": "Cymon",
  "config": {
    "check_tlp": true,
    "max_tlp": 1,
    "service": "Check_IP"
  },
  "configurationItems": [
    {
      "name": "key",
      "description": "API key for Cymon.io",
      "type": "string",
      "multi": false,
      "required": true
    }
  ]
}


